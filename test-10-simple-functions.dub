# shows the divisors of a given number, then prints '1234'

procedure test ()
    d = 1234
    print d
end

procedure divisors ()
    print "please enter a positive integer:"
    n = read_int
    d = 1
    while d <= n
        if n % d == 0
            print d
        end
        d = d + 1
    end
end

divisors()
test()

